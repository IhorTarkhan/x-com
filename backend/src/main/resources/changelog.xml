    <changeSet id="populate-positions" author="IhorTarkhan">
        <sql>
            DELETE
            FROM position
            WHERE id >= 0;

            INSERT INTO position (id, x, y)
            VALUES (1, 1, 1),
            (2, 2, 1),
            (3, 7, 3),
            (4, 5, 4),
            (5, 2, 4),
            (6, 3, 7),
            (7, 9, 4),
            (8, 3, 4),
            (9, 2, 4),
            (10, 7, 1);
        </sql>
    </changeSet>
    <changeSet id="populate-units" author="IhorTarkhan">
